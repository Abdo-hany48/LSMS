<!-- Views/Professor/ProfileProfessor.cshtml -->
@model Professor


<h2>Welcome to your Professor Profile</h2>

<p>Professor: @Model.Name</p>

<p>Professor: @Model.SSN</p>
<p>Professor: @Model.PhoneNumber</p>

@if (User.Identity.IsAuthenticated)
{
    <li><a asp-controller="Home" asp-action="Logout">Logout</a></li>
}
<!-- Add other profile information as needed -->

